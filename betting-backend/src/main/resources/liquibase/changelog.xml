<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.3.xsd">

    <preConditions>
        <dbms type="postgresql"/>
    </preConditions>

    <include file="migration/tables.sql" relativeToChangelogFile="true"/>
    <include file="migration/procedures.sql" relativeToChangelogFile="true"/>
    <include file="migration/triggers.sql" relativeToChangelogFile="true"/>
    <include file="migration/indexes.sql" relativeToChangelogFile="true"/>
    <include file="migration/data_mig.sql" relativeToChangelogFile="true"/>

</databaseChangeLog>